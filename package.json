{
  "name": "name-that-player",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "db:seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -r apps/server/src/database -d database.config.ts",
    "dev": "concurrently \"npm run server:start:dev\" \"npm run client:dev\"",
    "client:build": "nuxt build",
    "client:dev": "nuxt dev",
    "client:generate": "nuxt generate",
    "client:preview": "nuxt preview",
    "client:postinstall": "nuxt prepare",
    "server:build": "nest build",
    "server:format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "server:start": "nest start",
    "server:start:dev": "nest start --watch",
    "server:start:debug": "nest start --debug --watch",
    "server:start:prod": "node dist/apps/server/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/server/test/jest-e2e.json"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
