    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nuxt/devtools": "latest",
    "@pinia/nuxt": "^0.4.8",
    "@types/node": "^18",
    "concurrently": "^8.2.0",
    "mande": "^2.0.6",
    "nuxt": "^3.5.2",
    "papaparse": "^5.4.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "typeorm-extension": "^2.8.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@element-plus/nuxt": "^1.0.5",
    "@nestjs/cli": "^9.0.0",
